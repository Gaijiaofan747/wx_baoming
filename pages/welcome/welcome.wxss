.container .welcome{
    position: absolute;
    top: 70%;
    left: 0;
    width: 100%;
    height: 54rpx;
    color: rgba(82, 82, 82, 1);
    text-align: center;
    font-size: 32rpx;
    font-family: SourceHanSansSC-regular;
}

.welcome-text{
    font-size:36rpx;
}

.btn{
    position: absolute;
    left: 208rpx;
    top: 77%;
    width: 330rpx;
    height: 70rpx;
    border-radius: 60rpx;
    border-color:#fff;
    background: #fff;
    text-align: center;
    line-height: 70rpx;
    font-family: Microsoft Yahei;
    color: rgba(82, 82, 82, 1);
    font-size: 28rpx;
}

.btn:after{
